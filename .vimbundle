Plug 'tpope/vim-rsi'
Plug 'tpope/vim-sensible'
Plug 'tpope/vim-sleuth'
Plug 'tpope/vim-speeddating'
Plug 'tpope/vim-surround'
Plug 'tpope/vim-unimpaired'
Plug 'tpope/vim-vinegar'
Plug 'tpope/vim-vividchalk'
Plug 'tpope/vim-dispatch'
Plug 'tpope/vim-dadbod'
Plug 'vim-ruby/vim-ruby'
Plug 'wgibbs/vim-irblack'
Plug 'kana/vim-textobj-user'
Plug 'nelstrom/vim-textobj-rubyblock'
Plug 'exu/pgsql.vim'
Plug 'elixir-lang/vim-elixir'
Plug 'mxw/vim-jsx'
Plug 'go2null/vim-oil'
Plug 'scrooloose/nerdtree'
Plug 'ctrlpvim/ctrlp.vim'
Plug 'christoomey/vim-tmux-navigator'
Plug 'trevordmiller/nova-vim'
Plug 'altercation/vim-colors-solarized'
Plug 'tfnico/vim-gradle'
Plug 'brandonshega/material-vim'
Plug 'artur-shaik/vim-javacomplete2'
Plug 'gfontenot/vim-xcode'
Plug 'christoomey/vim-tmux-runner'
Plug 'sbdchd/neoformat'
Plug 'hashrocket/vim-hashrocket'
Plug 'airblade/vim-gitgutter'
Plug 'sheerun/vim-polyglot'
Plug 'diepm/vim-rest-console'
Plug '/usr/local/opt/fzf'
Plug 'junegunn/fzf', { 'do': { -> fzf#install() } }
Plug 'junegunn/fzf.vim'
Plug 'autozimu/LanguageClient-neovim', {
    \ 'branch': 'next',
    \ 'do': 'bash install.sh',
    \ }
Plug 'wellle/tmux-complete.vim'
Plug 'davidhalter/jedi-vim'
Plug 'ternjs/tern_for_vim'
Plug 'itchyny/lightline.vim'
Plug 'junegunn/seoul256.vim'
Plug 'dracula/vim', { 'as': 'dracula'}
Plug 'neoclide/coc.nvim', { 'branch': 'release' }
Plug 'Shougo/neomru.vim'
Plug 'connorholyday/vim-snazzy'
if has('nvim')
  Plug 'Shougo/denite.nvim', { 'do': ':UpdateRemotePlugins' }
else
  Plug 'Shougo/denite.nvim'
  Plug 'roxma/nvim-yarp'
  Plug 'roxma/vim-hug-neovim-rpc'
endif

" staticly check code and highlight errors (async syntastic replacement)
Plug 'w0rp/ale'

if filereadable(expand('~/.vimbundle.local'))
  source ~/.vimbundle.local
endif

" Initialize plugin system
call plug#end()
